{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.6.7",
  "main": "dist/index.html",
  "scripts": {
    "start": "dotenv -e .env serve dist -l 3333",
    "build": "vite-ssg build",
    "dev": "vite --port 3333",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks",
    "sizecheck": "npx vite-bundle-visualizer"
  },
  "dependencies": {
    "@unhead/vue": "^1.7.4",
    "@unocss/reset": "^0.53.6",
    "@vueuse/core": "^10.5.0",
    "@vueuse/head": "^1.3.1",
    "axios": "^1.5.1",
    "nprogress": "^0.2.0",
    "onu-ui": "1.0.14-beta.2",
    "pinia": "^2.1.6",
    "random-words": "^2.0.0",
    "serve": "^14.2.1",
    "vue": "^3.3.4",
    "vue-demi": "^0.14.6",
    "vue-i18n": "^9.5.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.8",
    "@iconify-json/carbon": "^1.1.21",
    "@types/markdown-it-link-attributes": "^3.0.2",
    "@types/nprogress": "^0.2.1",
    "@unocss/eslint-config": "^0.53.6",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue-macros/volar": "^0.12.3",
    "@vue/test-utils": "^2.4.1",
    "critters": "^0.0.16",
    "cross-env": "^7.0.3",
    "cypress": "^12.17.4",
    "cypress-vite": "^1.4.2",
    "eslint": "^8.51.0",
    "eslint-plugin-cypress": "^2.15.1",
    "https-localhost": "^4.7.1",
    "lint-staged": "^13.3.0",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-shiki": "^0.9.0",
    "pnpm": "^8.9.0",
    "sass": "1.32.12",
    "shiki": "^0.14.5",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.11.3",
    "typescript": "^5.2.2",
    "unocss": "^0.53.6",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "unplugin-vue-macros": "^2.6.1",
    "vite": "^4.4.11",
    "vite-bundle-visualizer": "^0.8.1",
    "vite-plugin-inspect": "^0.7.40",
    "vite-plugin-pages": "^0.31.0",
    "vite-plugin-pwa": "^0.16.5",
    "vite-plugin-vue-component-preview": "^1.1.6",
    "vite-plugin-vue-devtools": "^0.5.1",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vite-plugin-vue-markdown": "^0.23.8",
    "vite-plugin-webfont-dl": "^3.7.6",
    "vite-ssg": "^0.23.3",
    "vite-ssg-sitemap": "^0.5.1",
    "vitest": "^0.33.0",
    "vue-tsc": "^1.8.19"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.jsx": "eslint --fix",
    "*.ts": "eslint --fix",
    "*.tsx": "eslint --fix"
  }
}
